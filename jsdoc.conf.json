{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": "src/",
        "includePattern": ".+\\.js?$",
        "excludePattern": "spec+\\.js?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}